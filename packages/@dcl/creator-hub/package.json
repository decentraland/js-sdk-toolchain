{
  "name": "creator-hub",
  "description": "Decentraland Creator Hub",
  "version": "0.0.0-dev",
  "private": true,
  "type": "module",
  "author": {
    "email": "hello@decentraland.org",
    "name": "Decentraland Foundation",
    "url": "https://decentraland.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/decentraland/js-sdk-toolchain.git"
  },
  "main": "packages/main/dist/index.js",
  "scripts": {
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "cross-env MODE=production npm run build && electron-builder build --config electron-builder.cjs --dir",
    "compile:installer": "cross-env MODE=production npm run build && electron-builder build --config electron-builder.cjs --publish never",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.js",
    "test": "npm run test:main && npm run test:preload && npm run test:renderer && npm run test:shared",
    "test:e2e": "npm run build && vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:preload": "vitest run -r packages/preload --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "test:shared": "vitest run -r packages/shared --passWithNoTests",
    "watch": "node scripts/watch.js",
    "start": "npm run watch",
    "lint": "eslint . --ext js,mjs,cjs,ts,mts,cts,vue",
    "lint:fix": "eslint . --ext js,mjs,cjs,ts,mts,cts,vue --fix",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:renderer": "tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck:main && npm run typecheck:preload && npm run typecheck:renderer",
    "format": "npx prettier --write \"**/*.{js,ts,tsx,json}\""
  },
  "devDependencies": {
    "@dcl/hooks": "0.0.2",
    "@reduxjs/toolkit": "2.2.6",
    "@sentry/vite-plugin": "3.2.2",
    "@sentry/wizard": "4.2.0",
    "@types/cmd-shim": "5.0.2",
    "@types/http-server": "0.12.4",
    "@types/is-running": "2.1.2",
    "@types/node": "20.14.9",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/redux-logger": "3.0.13",
    "@types/segment-analytics": "0.0.38",
    "@types/semver": "7.5.8",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@vitejs/plugin-react": "4.3.1",
    "ansi-to-html": "0.7.2",
    "classnames": "2.5.1",
    "cross-env": "7.0.3",
    "dcl-catalyst-client": "21.8.1",
    "decentraland-ui2": "0.23.1",
    "electron": "37.2.3",
    "electron-builder": "26.0.12",
    "eslint": "8.57.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-vue": "9.27.0",
    "flat": "6.0.1",
    "happy-dom": "14.12.3",
    "playwright": "1.45.0",
    "prettier": "3.5.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-intl": "6.6.8",
    "react-redux": "9.1.2",
    "react-router-dom": "6.24.1",
    "redux-logger": "3.0.6",
    "typescript": "5.5.3",
    "unplugin-auto-expose": "0.3.0",
    "vite": "5.3.2",
    "vitest": "1.6.0"
  },
  "dependencies": {
    "@dcl/inspector": "file:../inspector",
    "@dcl/mini-rpc": "1.0.7",
    "@dcl/schemas": "11.10.4",
    "@ethersproject/hash": "5.7.0",
    "@segment/analytics-node": "2.1.2",
    "@sentry/electron": "6.1.0",
    "@sentry/react": "9.5.0",
    "cmd-shim": "6.0.3",
    "decentraland-connect": "7.1.0",
    "decentraland-crypto-fetch": "2.0.1",
    "deepmerge": "4.3.1",
    "electron-log": "5.1.6",
    "electron-updater": "6.6.7",
    "http-server": "14.1.1",
    "immer": "10.1.1",
    "is-running": "2.1.0",
    "npm": "10.8.1",
    "p-limit": "6.1.0",
    "rimraf": "6.0.1",
    "semver": "7.6.3",
    "tree-kill": "1.2.2"
  }
}
