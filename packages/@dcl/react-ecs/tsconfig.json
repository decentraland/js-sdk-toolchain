{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "stripInternal": true,
    "downlevelIteration": true,
    "preserveConstEnums": true,
    "outDir": "dist",
    "jsx": "react",
    "lib": ["ES2020"],
    "jsxFactory": "ReactEcs.createElement",
    "rootDir": "src",
    "types": []
  },
  "include": [
    "src"
  ],
  "exclude": [
    "dist"
  ]
}